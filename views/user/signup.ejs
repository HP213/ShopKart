							</div>
						</div>
					</div> -->

					<!-- Wishlist -->
					<div class="col-lg-10 col-9 order-lg-3 order-2 text-lg-left text-right">
						<div class="wishlist_cart d-flex flex-row align-items-center justify-content-end">


							<!-- Cart -->
              <div class="cart">
		            <div class="cart_container d-flex flex-row align-items-center justify-content-end">
		              <div class="cart_icon">
		                <img src="../images/images/cart.png" alt="">
		                <%if(session.cart){%>
		                <div class="cart_count">
		              <span class="badge"><%=session.cart.totalQty%></span>
		            <%}%></div>
		              </div>
		              <div class="cart_content">
		                <div class="cart_text"><a href="/cart">Cart</a></div>
		                <!-- <div class="cart_price">$85</div> -->
		              </div>
		            </div>
		          </div>
						</div>
					</div>
				</div>
			</div>
		</div>

		<!-- Main Navigation -->

		<nav class="main_nav">
			<div class="container">
				<div class="row">
					<div class="col">

						<div class="main_nav_content d-flex flex-row">


						</div>
					</div>
				</div>
			</div>
		</nav>

		<!-- Menu -->


	</header>

	<!-- Contact Info -->

	<div class="contact_info">
		<div class="container">
			<div class="row">
				<div class="col-lg-10 offset-lg-1">
					<div class="contact_info_container d-flex flex-lg-row flex-column justify-content-between align-items-between">



					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- Contact Form -->
	<div background="" >

	</div>

	<div class="contact_form photo" >
		<div class="container">
			<div class="row">
				<div class="col-lg-10 offset-lg-1">
					<div class="contact_form_container">
						<div class="contact_form_title">Sign Up</div>
            <% if(messages && messages.length > 0){%>
      <% for(message of messages){%>
      <div class=" alert alert-danger"><%=message%></div>
      <%}%>
      <%}%>

						<form action="/user/signup" method="post" id="contact_form">
							<div class="contact_form_inputs d-flex flex-md-row flex-column justify-content-between align-items-between">
								<input type="email" name="email" id="contact_form_email" class="contact_form_email input_field form-control" placeholder="Your Email" required="required" data-error="Email is required.">

							</div>
							<div class="contact_form_Password">
								<input type="password" name="password" id="contact_form_Password" class="contact_form_Password input_field form-control" placeholder="Password" required="required" data-error="Password is required.">

							</div>
              <input type="hidden" name="_csrf" value="<%=csrfToken%>">
							<div class="contact_form_button">
								<button type="submit" class="button contact_submit_button">Sign Up</button>
								<a href="/user/google" title="Google+" class="btn btn-googleplus btn-lg"><i class="fa fa-google-plus "></i> Google+</a>
							</div>


						</form>

					</div>
				</div>
			</div>
		</div>
		<div class="panel"></div>
	</div>





	<!-- Footer -->

	<footer class="footer">
		<div class="container">
			<div class="row">

				<div class="col-lg-3 footer_col">
					<div class="footer_column footer_contact">
						<div class="logo_container">
							<div class="logo"><a href="#">Shopkart</a></div>
						</div>
						<div class="footer_title">Always in Your Service.</div>

						<div class="footer_social">
							<ul>
								<li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
								<li><a href="#"><i class="fab fa-twitter"></i></a></li>
								<li><a href="#"><i class="fab fa-youtube"></i></a></li>
								<li><a href="#"><i class="fab fa-google"></i></a></li>
								<li><a href="#"><i class="fab fa-vimeo-v"></i></a></li>
							</ul>
						</div>
					</div>
				</div>


				<div class="col-lg-2">
					<div class="footer_column">

					</div>
				</div>

			</div>
		</div>
	</footer>

